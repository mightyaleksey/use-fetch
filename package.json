  ],
  "author": "Alexey Litvinov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sullenor/use-fetch/issues"
  },
  "homepage": "https://github.com/sullenor/use-fetch#readme",
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
